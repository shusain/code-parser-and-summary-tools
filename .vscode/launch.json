{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}/java-code-analyzer/app",
            "type": "java",
            "name": "JavaGithubSummarizer",
            "request": "launch",
            "mainClass": "sh.code.analyzer.App",
            "projectName": "app",
            "args": ["shusain/DungeonGame", "debug-out.md"]
        },
        {
            "name": "ts-node",
            "type": "node",
            "request": "launch",
            "args": [
                "${relativeFile}"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}